alg test_collection()
{
	println("\nCollection Tests:")

	x = {1, 2, 34}

	x.append(12, 543, 232.2332)

	println(x)

	x[0] = 13

	println(x[0])
	println("new x = ", x)
}

alg test_dictionary()
{
	println("\nDictionary Tests:")

	x = dict()

	x[1] = "hello"
	x[4.5] = "double"
	x["clear"] = "facts"

	println("x = ", x)
}
