g(x) = x^2 - x - 6

alg newton(r, f)
{
	less = null
	more = null

	y = f(r)

	println("y = ", y)
	println("g(1) = ", g(1))

	if (y > 0)
		more = r
	else
		less = r
	
	println("more = ", more)
	println("less = ", less)

	i = 0
	while (i < 10) {
		println("LOOP, i = ", i)

		if (i == 5) {
			println("\tI == 5!!")

			break
		}
		
		i++

		# break
	}
}

newton(1, g)